<!-- Done List -->
{% if done|length > 0 %}
<div class="container-fluid" style="font-size: 150%">
  <h2>Completed</h2>
  {% for d in done %}
  <div class="row">
    <div class="col-4 col-md-3 col-lg-2">{{ d.date }}</div>
    <div class="col-6 col-md-7 col-lg-9">{{ d.name }}</div>
    <div class="col-2 col-md-2 col-lg-1">
      <button
        class="btn btn-primary btn-block"
        type="button"
        data-toggle="collapse"
        data-target=".{{ type }}_{{ d.doc_id }}"
      >
        Edit
      </button>
    </div>
  </div>
  {% set collapse_class = type ~ '_' ~ d.doc_id %}
  {% with collapse_class=collapse_class, type=type, rowid=d.rowid, date=d.date, position=d.position, name=d.name, tbls=tbls %}
    {% include "sub/edit.html.j2" %}
  {% endwith %}
  <br />
  {% endfor %}
  <hr />
</div>
{% endif %}
